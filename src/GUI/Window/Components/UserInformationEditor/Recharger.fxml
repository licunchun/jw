<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefWidth="250.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.Components.UserInformationEditor.RechargerController">
   <children>
      <VBox prefWidth="250.0">
         <children>
            <AnchorPane prefWidth="250.0">
               <children>
                  <Label layoutX="93.0" text="充值">
                     <font>
                        <Font size="32.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="30.0" prefWidth="250.0">
               <children>
                  <Button fx:id="MoneyAdd10" layoutX="30.0" mnemonicParsing="false" prefWidth="55.0" text="10元" onAction="#doMoneyAdd10"/>
                  <Button fx:id="MoneyAdd50" layoutX="165.0" mnemonicParsing="false" prefWidth="55.0" text="50元" onAction="#doMoneyAdd20" />
                  <Button fx:id="MoneyAdd20" layoutX="98.0" mnemonicParsing="false" prefWidth="55.0" text="20元" onAction="#doMoneyAdd50" />
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="30.0" prefWidth="250.0">
               <children>
                  <Button fx:id="MoneyAdd100" layoutX="30.0" mnemonicParsing="false" prefWidth="55.0" text="100元" onAction="#doMoneyAdd100" />
                  <Button fx:id="MoneyAdd500" layoutX="165.0" mnemonicParsing="false" prefWidth="55.0" text="500元" onAction="#doMoneyAdd200" />
                  <Button fx:id="MoneyAdd200" layoutX="98.0" mnemonicParsing="false" prefWidth="55.0" text="200元" onAction="#doMoneyAdd500" />
               </children>
            </AnchorPane>
            <AnchorPane prefWidth="250.0">
               <children>
                  <Label fx:id="tips" textFill="RED" />
               </children></AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
