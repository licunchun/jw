<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="660.0" prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/21" fx:controller="GUI.Controller.Main.Admin.ManageUserPageController">
    <VBox prefHeight="660.0" prefWidth="1280.0">
        <AnchorPane prefHeight="60.0" prefWidth="1280.0">
            <AnchorPane layoutX="200.0" layoutY="5.0" prefHeight="50.0" prefWidth="300.0">
                <Label layoutX="14.0" layoutY="12.0" text="ID:">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
                <TextField fx:id="IDField" layoutX="70.0" layoutY="14.0" prefHeight="23.0"
                           prefWidth="200.0"/>
            </AnchorPane>
            <AnchorPane layoutX="600.0" layoutY="5.0" prefHeight="50.0" prefWidth="300.0">
                <Label layoutX="14.0" layoutY="12.0" text="姓名:">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
                <TextField fx:id="NameField" layoutX="70.0" layoutY="14.0" prefHeight="23.0"
                           prefWidth="200.0"/>
            </AnchorPane>
            <Button fx:id="SearchButton" layoutX="1150.0" layoutY="10.0" mnemonicParsing="false"
                    prefWidth="80.0" text="查询" onAction="#doSearch">
                <font>
                    <Font size="20.0"/>
                </font>
            </Button>
        </AnchorPane>
        <AnchorPane fx:id="TablePane" prefHeight="600.0" prefWidth="1280.0"/>
    </VBox>
</AnchorPane>
